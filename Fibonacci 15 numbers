def fibonacci():
    num1 = 0
    num2 = 1
    i = 0

    while i < 15:

        i = i + 1
        if i == 1:
            print(num1)

        elif i == 2:
            print(num2)

        elif i > 2:
            num3 = num1 + num2
            num1 = num2
            num2 = num3

            print(num3)


fibonacci()
